{
  "env": {
    "node": true
  },
  "extends": [
    "airbnb-base",
    "plugin:import/errors",
    "plugin:import/warnings",
    "plugin:import/recommended"
  ],
  "rules": {
    "no-param-reassign": ["error", {
      "props": false
    }],
    "no-continue": "off",
    "no-await-in-loop": "off",
    "no-restricted-syntax": "off",
    "arrow-body-style": "off"
  }
}