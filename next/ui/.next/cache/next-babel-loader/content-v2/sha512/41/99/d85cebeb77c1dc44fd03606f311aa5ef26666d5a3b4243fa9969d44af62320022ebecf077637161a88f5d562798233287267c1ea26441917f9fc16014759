{"ast":null,"code":"import ReactGA from 'react-ga';\nexport function scrollEA(data) {\n  var scrollToBlockEA = function scrollToBlockEA(block) {\n    ReactGA.event({\n      category: 'Scroll Block',\n      action: 'ScrollToBlock',\n      label: \"Scroll to \".concat(block)\n    });\n  };\n\n  var bloks = [];\n  document.querySelectorAll(\"[\".concat(data, \"]\")).forEach(function (e) {\n    bloks.push({\n      nameBlock: e.getAttribute(data),\n      distanceToTop: e.getBoundingClientRect().top\n    });\n  });\n  document.addEventListener('scroll', function () {\n    bloks.forEach(function (element, index) {\n      if (element && window.scrollY > element.distanceToTop - window.scrollY / 8) {\n        scrollToBlockEA(element.nameBlock);\n        delete bloks[index];\n      }\n    });\n  });\n}\nexport var clickSocialLinkEA = function clickSocialLinkEA(link) {\n  ReactGA.event({\n    category: 'Social Link',\n    action: 'clickToSocialLink',\n    label: \"\".concat(link)\n  });\n};\nexport var onChangeSlideEA = function onChangeSlideEA(sliderInfo) {\n  ReactGA.event({\n    category: \"Slider \".concat(sliderInfo.sliderName),\n    action: 'onSLideChange',\n    label: \"Current slide \".concat(sliderInfo.slide + 1)\n  });\n};\nexport var startProjectEA = function startProjectEA() {\n  ReactGA.event({\n    category: 'Start Project',\n    action: 'Click the button',\n    label: 'Click start project button'\n  });\n};\nexport var sendFormEA = function sendFormEA(values) {\n  ReactGA.event({\n    category: 'Send Form',\n    action: 'Send Form',\n    label: \"Validation was successful\"\n  });\n};","map":null,"metadata":{},"sourceType":"module"}