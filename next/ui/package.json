{
  "name": "ui",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "test:generators": "ts-node --project=./internals/ts-node.tsconfig.json ./internals/testing/test-generators.ts",
    "start:prod": "yarn build && serve -s build",
    "checkTs": "tsc --noEmit",
    "eslint": "eslint --ext js,ts,tsx",
    "lint": "yarn eslint -- src",
    "lint:fix": "yarn eslint -- --fix src",
    "lint:css": "stylelint src/**/*.css",
    "generate": "cross-env TS_NODE_PROJECT='./internals/ts-node.tsconfig.json' plop --plopfile internals/generators/plopfile.ts",
    "prettify": "prettier --write"
  },
  "dependencies": {
    "@reduxjs/toolkit": "1.5.1",
    "@testing-library/jest-dom": "5.1.1",
    "@testing-library/react": "10.0.1",
    "@types/fontfaceobserver": "0.0.6",
    "@types/jest": "25.1.4",
    "@types/node": "13.9.3",
    "@types/react": "16.9.25",
    "@types/react-dom": "16.9.3",
    "@types/react-helmet": "5.0.15",
    "@types/react-redux": "7.1.7",
    "@types/react-router-dom": "5.1.3",
    "@types/react-test-renderer": "16.9.2",
    "@types/styled-components": "5.0.1",
    "@types/testing-library__jest-dom": "5.0.2",
    "@types/webpack-env": "1.15.1",
    "@zeit/next-css": "1.0.1",
    "animate.css": "4.1.1",
    "axios": "0.21.1",
    "clean-html-js": "1.3.16",
    "country-list": "2.2.0",
    "cross-env": "7.0.2",
    "dayjs": "1.10.4",
    "emailjs-com": "2.6.3",
    "eslint-config-prettier": "6.10.1",
    "eslint-plugin-prettier": "3.1.2",
    "fontfaceobserver": "2.1.0",
    "formik": "2.1.5",
    "html-to-react": "1.4.5",
    "husky": "4.2.3",
    "i18next": "19.3.4",
    "i18next-browser-languagedetector": "4.0.2",
    "jest-styled-components": "7.0.2",
    "lint-staged": "10.0.8",
    "mailgun-js": "0.22.0",
    "next": "10.2.3",
    "next-compose-plugins": "2.2.1",
    "next-images": "1.7.0",
    "next-optimized-images": "2.6.2",
    "node-plop": "0.25.0",
    "plop": "2.6.0",
    "prettier": "2.0.1",
    "react": "16.14.0",
    "react-app-polyfill": "1.0.6",
    "react-country-flag": "2.3.0",
    "react-datepicker": "4.1.1",
    "react-dom": "16.13.0",
    "react-ga": "3.1.2",
    "react-helmet-async": "1.0.4",
    "react-hyphen": "1.4.0",
    "react-i18next": "11.3.4",
    "react-lines-ellipsis": "0.15.0",
    "react-multi-carousel": "2.6.3",
    "react-redux": "7.2.0",
    "react-scripts": "3.4.3",
    "react-scroll": "1.8.1",
    "react-select": "4.3.1",
    "react-slick": "0.27.6",
    "react-star-ratings": "2.3.0",
    "react-test-renderer": "16.13.0",
    "react-transition-group": "4.4.1",
    "redux-injectors": "1.3.0",
    "redux-saga": "1.1.3",
    "sanitize-html": "2.4.0",
    "sanitize.css": "11.0.0",
    "serve": "11.3.2",
    "shelljs": "0.8.3",
    "slick-carousel": "1.8.1",
    "styled-components": "5.0.1",
    "stylelint": "13.13.1",
    "stylelint-config-recommended": "3.0.0",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-processor-styled-components": "1.10.0",
    "suneditor": "2.39.0",
    "suneditor-react": "3.1.0",
    "swiper": "6.7.1",
    "ts-debounce": "3.0.0",
    "ts-node": "8.8.2",
    "typescript": "4.2.4",
    "use-local-storage-state": "10.0.0",
    "uuid": "8.3.0",
    "yup": "0.29.3"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "npm": ">=6.4.1",
    "node": ">=10.13.0"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "npm run eslint -- --fix"
    ],
    "*.{md,json}": [
      "prettier --write"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*/*.d.ts",
      "!src/**/*/Loadable.{js,jsx,ts,tsx}",
      "!src/**/*/types.ts",
      "!src/index.tsx",
      "!src/serviceWorker.ts"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    }
  },
  "devDependencies": {
    "@types/react-datepicker": "3.1.8",
    "@types/react-responsive": "8.0.2",
    "@types/react-select": "4.0.16",
    "@types/sanitize-html": "2.3.1",
    "@types/yup": "0.29.4",
    "babel-plugin-styled-components": "1.13.1",
    "css-loader": "5.2.4",
    "file-loader": "6.2.0",
    "postcss": "8.3.0",
    "url-loader": "4.1.1"
  }
}
